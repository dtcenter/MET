  <test name="point2grid_ascii2nc_surfrad_DW_PSP">
    <exec>&MET_BIN;/point2grid</exec>
    <param> \
      &OUTPUT_DIR;/ascii2nc/surfrad.nc \
      G212 \
      &OUTPUT_DIR;/point2grid/surfrad_to_G212.nc \
